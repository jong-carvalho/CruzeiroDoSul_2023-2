algoritmo name
    início
        inteiro num

        escreva("Digite um número: ")
        leia num 

        se(num % 2 == 0)
            escreva("O número digitado é par")
        senao se(num % 2 != 0 )
            escreva("O número digitado é ímpar")
        senao
            escreva("Entrada inválida")
    fim